        <jsScript_script>//Script here
var fechaValida=null;

var str = per_fec_nacimiento;
var res = str.split("/");
var anio=res[0];
var mes=res[1];
var dia=res[2];

//Validar formato yyyy/MM/dd
if(dia==undefined || dia==null || isNaN(Number(dia))){
	dia="0";
}
if(mes==undefined || mes==null || isNaN(Number(mes))){
	mes="0";
}
if(anio==undefined || anio==null || isNaN(Number(anio))){
	anio="0";
}
//validar dia
if(Number(dia)>31 &amp;&amp; Number(anio)&lt;=31){
	//interncambiar dia por anio
	var diaAux=dia;
	dia=anio;
	anio=diaAux;
}
//validar mes
if(Number(mes)>12){
	var mes="0";
}
//obtener fecha segun parametros
if(dia!="0" &amp;&amp; mes !="0" &amp;&amp; anio!="0"){
	fechaValida=new Date(Number(anio), Number(mes) - 1, Number(dia));
}

</jsScript_script>
